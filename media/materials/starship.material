import PbrEmissivePass from "base.material"

material StarShip3_RTSS
{
    technique
    {
        pass : PbrEmissivePass
        {
            set $diffuse_texture "assets/starships/starship3/textures/main_albedo.jpeg"
            set $emission_texture "assets/starships/starship3/textures/main_emission.jpeg"
            set $normal_texture "assets/starships/starship3/textures/main_normal.jpeg"
            set $metalRoughness_texture "assets/starships/starship3/textures/main_metalRoughness.jpeg"
        }
    }
}

